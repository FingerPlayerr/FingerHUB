-- Gui to Lua
-- Version: 3.2

-- Instances:

local UI = Instance.new("ScreenGui")
local UI_2 = Instance.new("Frame")
local BackGround = Instance.new("Frame")
local Container = Instance.new("Frame")
local Padding = Instance.new("UIPadding")
local UIGridLayout = Instance.new("UIGridLayout")
local useallcodes = Instance.new("TextButton")
local tpwinter = Instance.new("TextButton")
local tporiginalmap = Instance.new("TextButton")
local tpvolcano = Instance.new("TextButton")
local tpbeach = Instance.new("TextButton")
