local Window = Instance.new("TextLabel")

--Properties:

UI.Name = "UI"
UI.Parent = game.CoreGui
UI.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

UI_2.Name = "UI"
UI_2.Parent = UI
UI_2.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
UI_2.BorderSizePixel = 0
UI_2.Position = UDim2.new(0, 15, 0, 15)
UI_2.Size = UDim2.new(0, 190, 0, 30)

BackGround.Name = "BackGround"
BackGround.Parent = UI_2
BackGround.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
BackGround.BorderSizePixel = 0
BackGround.Position = UDim2.new(0, 0, 1, 0)
BackGround.Size = UDim2.new(0, 190, 0, 30)

Container.Name = "Container"
Container.Parent = UI_2
Container.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Container.BorderSizePixel = 0
Container.Position = UDim2.new(0, 0, 1, 0)
Container.Size = UDim2.new(0, 190, 0, 663)

Padding.Name = "Padding"
Padding.Parent = Container
Padding.PaddingLeft = UDim.new(0, 10)
Padding.PaddingTop = UDim.new(0, 5)

UIGridLayout.Parent = Container
UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIGridLayout.CellSize = UDim2.new(0, 170, 0, 50)

useallcodes.Name = "useallcodes"
useallcodes.Parent = Container
useallcodes.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
useallcodes.BackgroundTransparency = 1.000
useallcodes.Size = UDim2.new(0, 200, 0, 50)
useallcodes.Font = Enum.Font.GothamBlack
useallcodes.Text = "Redeem Codes"
useallcodes.TextColor3 = Color3.fromRGB(255, 0, 0)
useallcodes.TextSize = 24.000
useallcodes.TextWrapped = true
useallcodes.MouseButton1Down:connect(function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/FingerPlayerr/FingerHUB/main/SpeedCityCodes"))()
end)

